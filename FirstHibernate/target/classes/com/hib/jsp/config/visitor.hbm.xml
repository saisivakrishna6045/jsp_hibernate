<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.hib.jsp.beans">
	<class table="VISITORS" name="com.hib.jsp.beans.Visitor">
		<id name="visitorId" column="VISITOR_ID">
			<generator class="assigned"/>
		</id>
		<property name="visitorName" column="VISITOR_NAME"/>
		<property name="visitorMailId" column="VISITOR_MAIL_ID"/>
		<property name="visitorMobileNo" column="VISITOR_MOBILE_NO"/>
		<property name="country" column="COUNTRY"/>
	</class>
</hibernate-mapping>